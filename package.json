{
  "name": "redux-epic-middleware",
  "declaration": true,
  "version": "1.0.1",
  "description": "A Redux middleware for handling side effects based on dispatched actions.",
  "main": "lib/cjs/index.js",
  "module": "lib/es6/index.js",
  "types": "lib/es6/index.d.ts",
  "scripts": {
    "update": "npm-check-updates -u && npm i",
    "test": "jest",
    "build": "tsc --project ./tsconfig.json && tsc --project ./tsconfig_cjs.json",
    "watchtest": "jest --watchAll",
    "watchlib": "tsc --watch",
    "dev": "concurrently \"npm run watchlib\" \"npm run watchtest\""
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/grobakh/redux-epic.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "peerDependencies": {
    "redux": "4.2.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "concurrently": "8.2.1",
    "eslint": "8.48.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-package-json": "0.1.4",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.6.4",
    "npm-check-updates": "16.13.3",
    "prettier": "3.0.3",
    "redux": "4.2.1",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  },
  "include": [
    "lib"
  ],
  "exclude": [
    "node_modules"
  ]
}
